{
  "root": "apps/mobile-app",
  "sourceRoot": "apps/mobile-app/src",
  "projectType": "application",
  "targets": {
    "analyze": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter analyze",
        "cwd": "apps/mobile-app"
      }
    },
    "clean": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter clean",
        "cwd": "apps/mobile-app"
      }
    },
    "format": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter format apps/mobile-app/*",
        "cwd": "apps/mobile-app"
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter test",
        "cwd": "apps/mobile-app"
      }
    },
    "assemble": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter assemble",
        "cwd": "apps/mobile-app"
      }
    },
    "attach": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter attach",
        "cwd": "apps/mobile-app"
      }
    },
    "drive": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter drive",
        "cwd": "apps/mobile-app"
      }
    },
    "genL10n": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter gen-l10n",
        "cwd": "apps/mobile-app"
      }
    },
    "install": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter install",
        "cwd": "apps/mobile-app"
      }
    },
    "run": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter run",
        "cwd": "apps/mobile-app"
      }
    }
  },
  "tags": []
}
